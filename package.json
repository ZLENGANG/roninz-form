{
  "name": "roninz-form",
  "private": false,
  "version": "0.0.18",
  "type": "module",
  "author": "Roninz <1437004632@qq.com.com>",
  "license": "MIT",
  "description": "基于element-plus的数据驱动表单组件，常用于可视化项目或表单较多的中台项目",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ZLENGANG/roninz-form"
  },
  "main": "./dist/roninz-form.umd.cjs",
  "module": "./dist/roninz-form.js",
  "exports": {
    "./css": "./dist/style.css",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/roninz-form.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/roninz-form.umd.cjs"
      }
    }
  },
  "scripts": {
    "dev": "vite serve examples  --config vite-demo.config.ts",
    "build": "vue-tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "element-plus": "^2.4.4",
    "lodash": "^4.17.21",
    "vite-plugin-dts": "^3.7.1",
    "vue": "^3.3.11"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@vitejs/plugin-vue": "^4.5.2",
    "less": "^4.2.0",
    "typescript": "^5.2.2",
    "unplugin-element-plus": "^0.8.0",
    "vite": "^5.0.8",
    "vite-plugin-dts": "^3.7.2",
    "vue-tsc": "^1.8.25"
  },
  "keywords": [
    "Vue3",
    "TS",
    "Vite",
    "Form",
    "Components",
    "Element-Plus"
  ]
}